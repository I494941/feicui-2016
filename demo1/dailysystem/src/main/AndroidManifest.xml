<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="contacts.android.wjf.android06.feicuiedu.com.dailysystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>

        <activity android:name=".main.RegisterActivity"/>
        <activity android:name=".main.LoginActivity"/>
        <activity android:name=".main.ReadActivity"/>
        <activity android:name=".main.DetailActivity"/>
        <activity android:name=".main.WriteActivity"/>


    </application>

</manifest>
