<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.wjf.android06.feicuiedu.com.test">

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />
    <!-- 清理进程权限 -->
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <!-- 读写SDCard权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 读取电话状态权限 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 相机权限 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <!-- WIFI权限 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 蓝牙权限 -->
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <!-- 打电话 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- 网络 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 开机启动 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
    <!-- android:theme="@style/AppTheme.NoActionBar"-->
        <activity android:name=".activity.LeadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TellistActivity" />
        <activity android:name=".activity.AboutActivity" />
        <activity android:name=".activity.FilemgrActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.LogoActivity" />
        <activity android:name=".activity.PhonemgrActivity" />
        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".activity.SoftmgrActivity" />
        <activity android:name=".activity.SoftmgrAppshowActivity"/>
        <activity android:name=".activity.TelmgrActivity" />
        <activity android:name=".activity.SpeedupActivity" />
        <activity android:name=".activity.AppShowActivity" />


    </application>

</manifest>